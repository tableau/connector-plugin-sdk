<?xml version='1.0' encoding='utf-8' ?>
<tdr class='sqlite_extract'>
<connection-resolver>
    <connection-builder>
        <script file='connectionBuilder.js'/>
    </connection-builder>
    <connection-normalizer>
        <required-attributes>
            <attribute-list>
            <attr>server</attr>
            <attr>authentication</attr>
            </attribute-list>
        </required-attributes>
    </connection-normalizer>
</connection-resolver>

<driver-resolver>
    <driver-match >
      <driver-name type='exact'>SQLite3 ODBC Driver</driver-name>
    </driver-match>
</driver-resolver>
</tdr>
